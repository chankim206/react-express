{
  "name": "react-express",
  "version": "1.0.0",
  "description": "React and Express Boilerplate",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "concurrently \"node run server.js\" \"npm run start-client.js\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Young Kim",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^5.0.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.1"
  }
}
